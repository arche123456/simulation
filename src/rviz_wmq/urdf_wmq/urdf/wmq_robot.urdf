<!-- 
    创建一个盒装机器人
    长：1.0米
    宽：0.6米
    高：0.4米
 -->
 <!-- 根标签 -->
 <robot name="robot_wmq">
    <!-- 连杆 -->
    <link name="link_wmq">
        <!-- 可视化 -->
        <visual>
            <!-- 几何外观 -->
            <geometry>
                <!-- 长方体 -->
                <box size="1.0 0.6 0.4"/>
            </geometry>
        </visual>
    </link>
 </robot>